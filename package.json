{
	"name": "tem-stack",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently --kill-others \"npm run db:generate && next dev --port 4242\" \"npm run db:studio\"",
		"build": "npm run db:generate && next build",
		"lint": "next lint",
		"analyze": "ANALYZE=true next build",
		"format": "eslint --fix . && prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"format:check": "eslint . && prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"postinstall": "npm run db:generate",
		"// ========== Database Generation ==========": "",
		"db:generate": "npm run db:site:generate",
		"db:site:generate": "prisma generate --schema ./src/prisma/site/schema",
		"// ========== Development Migrations ==========": "",
		"db:migrate:dev": "npm run db:site:migrate-dev",
		"db:site:migrate-dev": "prisma migrate dev --schema ./src/prisma/site/schema",
		"// ========== Production Deployment ==========": "",
		"db:migrate:deploy": "npm run db:site:migrate-deploy ",
		"db:site:migrate-deploy": "prisma migrate deploy --schema ./src/prisma/site/schema && npm run db:site:generate",
		"// ========== Database Push (Dev Only) ==========": "",
		"db:push": "npm run db:site:push",
		"db:site:push": "prisma db push --schema ./src/prisma/site/schema",
		"// ========== Database Pull ==========": "",
		"db:pull": "npm run db:site:pull",
		"db:site:pull": "prisma db pull --schema ./src/prisma/site/schema",
		"// ========== Prisma Studio ==========": "",
		"db:studio": "concurrently \"npm run db:site:studio\"",
		"db:site:studio": "prisma studio --schema ./src/prisma/site/schema --port 5555 --browser none",
		"// ========== Migration Status ==========": "",
		"db:status": "npm run db:site:status",
		"db:site:status": "prisma migrate status --schema ./src/prisma/site/schema",
		"// ========== Database Reset (Dev Only) ==========": "",
		"db:reset": "npm run db:site:reset",
		"db:site:reset": "prisma migrate reset --schema ./src/prisma/site/schema",
		"// ========== Migration Diff Tools ==========": "",
		"db:site:diff": "prisma migrate diff --from-schema-datamodel ./src/prisma/site/schema --to-schema-datasource ./src/prisma/site/schema --script",
		"db:site:diff-from-prod": "prisma migrate diff --from-url \"$SITE_DATABASE_URL\" --to-schema-datamodel ./src/prisma/site/schema --script",
		"// ========== Migration Resolution Helpers ==========": "",
		"db:site:resolve-applied": "echo 'Usage: npm run db:site:resolve-applied -- MIGRATION_NAME' && prisma migrate resolve --applied --schema ./src/prisma/site/schema",
		"db:site:resolve-rollback": "echo 'Usage: npm run db:site:resolve-rollback -- MIGRATION_NAME' && prisma migrate resolve --rolled-back --schema ./src/prisma/site/schema",
		"// ========== Migration Utilities ==========": "",
		"migration:repair": "node scripts/migration/repair.js",
		"migration:check": "npm run db:status",
		"migration:validate": "npm run db:site:diff",
		"// ========== CI/CD Helpers ==========": "",
		"ci:migrate": "npm run db:migrate:deploy",
		"ci:validate": "npm run db:status && npm run build"
	},
	"dependencies": {
		"@ai-sdk/openai": "^2.0.53",
		"@ai-sdk/openai-compatible": "^1.0.22",
